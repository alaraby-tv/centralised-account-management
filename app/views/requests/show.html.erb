<div class="page-header">
  <%= link_to requests_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_request_path(@request), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show request</h1>
</div>

<dl class="dl-horizontal">
  <dt>End User:</dt>
  <dd><%= @request.end_user.name %></dd>

  <dt>Requester:</dt>
  <dd><%= @request.requester.name %></dd>

  <dt>Access accounts:</dt>
    <% @request.access_accounts.each do |access_account| %>
  <dd><%= access_account.name %></dd>
  <% end %>

  <dt>Note:</dt>
  <dd><%= @request.note %></dd>

</dl>
